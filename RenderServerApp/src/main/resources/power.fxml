<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TextArea?>
<VBox fx:controller="application.controller.AppController" xmlns="http://javafx.com/fxml" xmlns:fx="http://javafx.com/fxml"
    styleClass="mainElementCustomClass">
	<children>
		<Label style="-fx-font-size:40" prefWidth="400" text="Render Server App"/>	
		<HBox prefWidth="400">	
			<Label style="-fx-font-size:16" prefWidth="75" text="Command"/>
			<TextField id="textFieldCommand" prefWidth="200" />
			<Button id="buttonSend" prefWidth="150" text="Send"/>	
		</HBox>
		<TextArea id="textAreaOutputConsole" prefWidth="400" prefHeight="300" />			
	</children>
</VBox>