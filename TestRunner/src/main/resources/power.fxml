<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.web.HTMLEditor?>
<VBox fx:controller="application.controller.AppController" xmlns="http://javafx.com/fxml" xmlns:fx="http://javafx.com/fxml"
    styleClass="mainElementCustomClass">
	<children>
		<Label style="-fx-font-size:40" prefWidth="400" text="Test Runner App"/>	
		<HBox prefWidth="400">	
			<Label style="-fx-font-size:16" prefWidth="75" text="Command"/>
			<TextField id="textFieldCommand" prefWidth="200" />
			<Button id="buttonSend" prefWidth="150" text="Send"/>	
		</HBox>
		<HTMLEditor id="textAreaOutputConsole" htmlText="&lt;html&gt;&lt;head&gt;&lt;/head&gt;&lt;body contenteditable=&quot;true&quot;&gt;&lt;h1&gt;Test Output&lt;/h1&gt;&lt;/body&gt;&lt;/html&gt;" prefHeight="500" prefWidth="900" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" />	
		<TableView id="tableViewTestResults" prefWidth="400" prefHeight="300">
		</TableView>
		
	</children>
</VBox>